# Snyk (https://snyk.io) policy file, patches or ignores known vulnerabilities.
version: v1.14.1
ignore:
  SNYK-JS-ANSIREGEX-1583908:
    - vsce > keytar > prebuild-install > npmlog > gauge > strip-ansi > ansi-regex:
      reason: Dependency Requirements
    - vsce > keytar > prebuild-install > npmlog > gauge > string-width > strip-ansi > ansi-regex:
      reason: Dependency Requirements
    - vsce > keytar > prebuild-install > npmlog > gauge > wide-align > string-width > strip-ansi > ansi-regex:
      reason: Dependency Requirements

# patches apply the minimum changes required to fix a vulnerability
patch:
  SNYK-JS-LODASH-567746:
    - vsce > lodash:
        patched: '2020-04-30T22:24:11.456Z'
    - vsce > cheerio > lodash:
        patched: '2020-04-30T22:24:11.456Z'
